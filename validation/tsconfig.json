{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "dist",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "lib": ["es6", "dom"],
    "esModuleInterop": true
  },
  "include": [
    "compare.ts"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "output",
    "accname_libs_source_code"
  ],
  "types": [
    "@types/puppeteer",
    "@types/devtools-protocol"
  ],
}
