{
  "name": "accessible-name",
  "version": "0.0.1",
  "description": "This library will contain a TypeScript implementation of the [Accessible Name Computation](https://www.w3.org/TR/accname-1.1/).",
  "main": "accessible-name.js",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.11",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "clang-format": "^1.4.0",
    "gts": "^2.0.2",
    "husky": "^4.2.5",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "jsdom": "^16.2.2",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.3.1",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "^5.0.3",
    "karma-typescript-es6-transform": "^5.0.3",
    "lit-html": "^1.2.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "files": [
    "!/src/lib/*_test.ts"
  ],
  "scripts": {
    "build": "tsc",
    "test": "karma start",
    "test-headless": "karma start --single-run --browsers ChromeHeadless,FirefoxHeadless",
    "check": "npx gts check src/**/*.ts validation/*.ts",
    "clean": "npx gts clean src/**/*.ts validation/*.ts",
    "compile": "tsc",
    "fix": "npx gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "format": "clang-format --version; find src | grep '\\.js$\\|\\.ts$' | xargs clang-format --style=file -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/googleinterns/accessible-name.git"
  },
  "keywords": [],
  "author": "Ois√≠n Nolan",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/googleinterns/accessible-name/issues"
  },
  "homepage": "https://github.com/googleinterns/accessible-name#readme",
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "npm run fix src/*.ts src/lib/*.ts validation/*.ts"
    }
  }
}
